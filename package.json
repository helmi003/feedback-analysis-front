{
  "name": "feedback-analysis-front",
  "private": true,
  "version": "0.0.40",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "sonar": "sonar-scanner",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "release": "standard-version",
    "release:production": "standard-version -t production-",
    "release:production:major": "standard-version --release-as major -t production-",
    "release:production:minor": "standard-version --release-as minor -t production-",
    "release:production:patch": "standard-version --release-as patch -t production-",
    "release:staging": "standard-version -t staging-",
    "release:staging:major": "standard-version --release-as major -t staging-",
    "release:staging:minor": "standard-version --release-as minor -t staging-",
    "release:staging:patch": "standard-version --release-as patch -t staging-"
  },
  "dependencies": {
    "antd": "^5.26.1",
    "axios": "^1.10.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-intl": "^7.1.11",
    "react-query": "^3.39.3",
    "react-router-dom": "^7.6.2",
    "recharts": "^2.15.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "sonar-scanner": "^3.1.0",
    "standard-version": "^9.5.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
